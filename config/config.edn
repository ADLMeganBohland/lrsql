{:database
 #profile
  {:test    {:db-type   "h2:mem"
             :db-name   "example"
             :db-host   "localhost"
             :db-port   8080
             :db-schema "lrsql"}
   :dev     {:db-type   "h2"
             :db-name   "example"
             :db-host   "localhost"
             :db-port   8080
             :db-schema "lrsql"}
   :default {:db-type   #or [#env DB_TYPE "h2:mem"]
             :db-name   #or [#env DB_NAME "example"]
